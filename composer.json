{
  "name": "zlodes/psalm-stub-bug",
  "description": "Psalm stub bug demo app",
  "type": "project",
  "authors": [
    {
      "name": "Zlatoslav Desyatnikov",
      "email": "px.zlodes@gmail.com"
    }
  ],
